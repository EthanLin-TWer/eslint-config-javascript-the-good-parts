{
  "name": "eslint-config-javascript-the-good-parts",
  "version": "1.0.0",
  "description": "Best JavaScript practices rules along with JavaScript: The Good Parts book",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha --require babel-register test",
    "bump:patch": "npm version patch -m \"[$(git config user.name)] ğŸ”–Version: patch upgrade to %s and published to npm\"",
    "bump:minor": "npm version minor -m \"[$(git config user.name)] ğŸ”–Version: minor upgrade to %s and published to npm\"",
    "bump:major": "npm version major -m \"[$(git config user.name)] ğŸ”–Version: major upgrade to %s and published to npm\"",
    "publish:patch": "npm run bump:patch && npm publish",
    "publish:minor": "npm run bump:minor && npm publish",
    "publish:major": "npm run bump:major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linesh-simplicity/eslint-config-javascript-the-good-parts.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "javascript",
    "es6",
    "best-practices"
  ],
  "author": "Linesh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linesh-simplicity/eslint-config-javascript-the-good-parts/issues"
  },
  "homepage": "https://github.com/linesh-simplicity/eslint-config-javascript-the-good-parts#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "iconv-lite": "^0.4.17",
    "mocha": "^3.4.2",
    "prettier": "^1.14.3"
  },
  "peerDependencies": {
    "eslint": ">= 3",
    "eslint-plugin-import": "^2.14.0"
  }
}
